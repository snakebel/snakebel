{
  "name": "@snakebel/news-card",
  "version": "0.0.2",
  "description": "Snakebel News Card",
  "main": "dist/newscard.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": ["dist/"],
  "browser": "dist/newscard.js",
  "scripts": {
    "build": "stencil build",
    "test": "jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snakebel/snakebel.git"
  },
  "dependencies": {
    "@stencil/core": "0.2.3",
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "@snakebel/util": "0.0.1"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "lerna": "^2.8.0",
    "prettier": "^1.10.2",
    "tslint": "^5.9.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$":
        "../../node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "json", "jsx"]
  },

  "author": "Santhosh Beerelli",
  "license": "MIT"
}
