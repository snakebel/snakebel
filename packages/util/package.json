  },
  "devDependencies": {
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "prettier": "^1.10.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
